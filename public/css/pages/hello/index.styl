#test
  width: 60em
  height: 200px;
  border: 1px solid red
  position: relative;
  .list-container
    width: 100%;
    height: 100%
    .list-wrap
      width: 100%
      height: 100%
      ul
        height: 100%

  .grids-item
    height: 100%;
    .list-container
      position: absolute;
      overflow: hidden;
      z-index: 1
      .list-wrap
        height: auto;
        position: absolute;
        z-index: 1
        ul
          display: block;
          li
            text-align: left;
            display: block;
