iconfonts()
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;

.j-rate
  font-size: 0
  li
    cursor: pointer
    display: inline-block;
    margin-right: 8px
    -webkit-transition: all .3s;
    transition: all .3s;
    &:hover,&:focus,&.j-rate-full
      i
        &:before
          content: "\e80a"
    i
      iconfonts()
      font-size: 24px
      &:before
        content: "\e7f1"
        color: #ff8400
