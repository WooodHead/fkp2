.markdown-body
  -webkit-text-size-adjust: 100%;
  text-size-adjust: 100%;
  color: #333;
  overflow: hidden;
  font-family: 'lucida grande', 'lucida sans unicode', lucida, helvetica, 'Hiragino Sans GB', 'Microsoft YaHei', 'WenQuanYi Micro Hei', sans-serif;
  font-size: 16px;
  line-height: 1.5;
  word-wrap: break-word;
  *
    box-sizing: border-box;

  >*
    &:first-child
      margin-top: 0 !important;

  >*
    &:last-child
      margin-bottom: 0 !important;

  a
    background-color: transparent;
    color: #4078c0;
    text-decoration: none;
    &:active, &:hover
      outline: 0;
      text-decoration: underline;
    &:not([href])
      color: inherit;
      text-decoration: none;

  p
    text-indent: 1em;
    &:first-child
        text-indent: 0;

  p, blockquote, ul, ol, dl, table, pre
    margin-top: 0;
    margin-bottom: 1em;
    font-size: 0.875em;

  img
    border: 0;
    max-width: 100%;

  li>a
    color: #3194d0;

  li>p
    margin-top: 16px;

  strong
    font-weight: bold;

  hr
    box-sizing: content-box;
    height: 0;
    //
    height: 0;
    margin: 10px 0;
    overflow: hidden;
    background: transparent;
    border: 0;
    border-bottom: 1px solid #ddd;
    &:before
      display: table;
      content: "";

  .octicon
    font: normal normal normal 16px/1 octicons-anchor;
    display: inline-block;
    text-decoration: none;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;

  .octicon-link
    &:before
      content: '\f05c';

  hr
    height: 0;
    margin: 10px 0;
    overflow: hidden;
    background: transparent;
    border: 0;
    border-bottom: 1px solid #ddd;
    &:before
      display: table;
      content: "";
    &:after
      display: table;
      clear: both;
      content: "";

  .anchor
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    padding-right: 6px;
    padding-left: 30px;
    margin-left: -30px;
    &:focus
      outline: none;

  h1, h2, h3, h4, h5, h6
    position: relative;
    margin-top: 15px;
    margin-bottom: 15px;
    font-weight: bold;
    line-height: 1.1;
    //
    margin-top: 3em;
    margin-bottom: 16px;
    line-height: 1.4;
    .octicon-link
      display: none;
      color: #000;
      vertical-align: middle;
    &:hover
      .anchor
        padding-left: 8px;
        margin-left: -30px;
        text-decoration: none;
        .octicon-link
          display: inline-block;

  h1
    font-size: 2em;
    margin: 0.67em 0;
    font-size: 30px;

    margin-top: 0;
    padding: 1em 0;
    font-size: 1.8em;
    line-height: 1.2;
    border-bottom: 1px solid #eee;
    .anchor
      line-height: 1;
    small
      font-size: 12px;
      font-weight: 100;
      color: #a6a6a6;
      vertical-align: bottom;

  h2
    font-size: 21px;
    padding-bottom: 0.3em;
    font-size: 1.6em;
    line-height: 1.225;
    border-bottom: 1px solid #eee;
    .anchor
      line-height: 1;

  h3
    font-size: 16px;
    font-size: 1.45em;
    line-height: 1.33;
    .anchor
      line-height: 1.2;


  h4
    margin-top: 0;
    font-size: 14px;
    font-size: 1.25em;
    .anchor
      line-height: 1.2;


  h5
    font-size: 12px;
    font-size: 1.08em;
    .anchor
      line-height: 1.1;


  h6
    font-size: 11px;
    font-size: 1em;
    color: #777;
    .anchor
      line-height: 1.1;

  h4, h5, h6
    margin-top: 1.5em;

  pre
    overflow: auto;
    margin-top: 0;
    margin-bottom: 0;
    font: 12px Consolas, "Liberation Mono", Menlo, Courier, monospace;

  code, kbd, pre
    font-family: monospace, monospace;
    font-size: 0.6em;

  code
    font-family: Consolas, "Liberation Mono", Menlo, Courier, monospace;
    font-size: 1em;
    padding: 0;
    margin: 0;
    font-size: 1.1em;
    background-color: #f9f2f4;
    border-radius: 3px;
    margin:0 .4em;
    color: #c7254e;
    &:before, &:after
      letter-spacing: -0.2em;
      content: "\00a0";


  input
    font: 13px/1.4 Helvetica, arial, nimbussansl, liberationsans, freesans, clean, sans-serif, "Segoe UI Emoji", "Segoe UI Symbol";
    color: inherit;
    font: inherit;
    margin: 0;
    line-height: normal;

  input[disabled]
    cursor: default
    &[type="checkbox"]
      box-sizing: border-box;
      padding: 0;


  table
    border-collapse: collapse;
    border-spacing: 0;

  td, th
    padding: 0;


  table
    width: 100%;
    overflow: auto;
    word-break: normal;
    word-break: keep-all;
    th
      font-weight: bold;
      padding: 6px 13px;
      border: 1px solid #ddd;
    td
      padding: 6px 13px;
      border: 1px solid #ddd;
    tr
      background-color: #fff;
      border-top: 1px solid #ccc;
      &:nth-child(2n)
        background-color: #f8f8f8;





  ul, ol
    font-size: 1em;
    padding: 0;
    margin-top: 0;
    margin-bottom: 0;
    ol
      list-style-type: lower-roman;
      ol
        list-style-type: lower-alpha;
    ul
      ol
        list-style-type: lower-alpha;



  dl
    padding: 0;
    dt
      padding: 0;
      margin-top: 16px;
      font-size: 1em;
      font-style: italic;
      font-weight: bold;
    dd
      margin-left: 0;
      padding: 0 16px;
      margin-bottom: 16px;



  select
    &::-ms-expand
      opacity: 0;


  hr
    height: 1px;
    padding: 0;
    margin: 0 0;
    background-color: #e7e7e7;
    border: 0 none;


  ul, ol
    padding-left: 0;
    margin-left: 1.5em;
    ul, ol
      margin-top: 0;
      margin-bottom: 0;

  li
    font-size: 14px;


  blockquote
    padding: 10px 15px;
    color: #777;
    border-left: 0.5em solid #ddd;
    background-color: #fafafa;
    &:first-child
      margin-top: 0;
    &:last-child
      margin-bottom: 0;






  pre>code {
    padding: 0;
    margin: 0;
    font-size: 1em;
    word-break: normal;
    white-space: pre;
    background: transparent;
    border: 0;
  }

  .highlight {
    margin-bottom: 16px;
  }

  .highlight pre,
  pre {
    padding: 16px;
    overflow: auto;
    font-size: .75em;
    line-height: 1.45;
    background-color: #f7f7f7;
    border-radius: 3px;
  }

  .highlight pre {
    margin-bottom: 0;
    word-break: normal;
  }

  pre {
    word-wrap: normal;
  }

  pre code {
    display: inline;
    max-width: initial;
    padding: 0;
    margin: 0;
    overflow: initial;
    line-height: inherit;
    word-wrap: normal;
    background-color: transparent;
    border: 0;
  }

  pre code:before,
  pre code:after {
    content: normal;
  }

  kbd {
    display: inline-block;
    padding: 3px 5px;
    font-size: 11px;
    line-height: 10px;
    color: #555;
    vertical-align: middle;
    background-color: #fcfcfc;
    border: solid 1px #ccc;
    border-bottom-color: #bbb;
    border-radius: 3px;
    box-shadow: inset 0 -1px 0 #bbb;
  }

  .pl-c {
    color: #969896;
  }

  .pl-c1,
  .pl-s .pl-v {
    color: #0086b3;
  }

  .pl-e,
  .pl-en {
    color: #795da3;
  }

  .pl-s .pl-s1,
  .pl-smi {
    color: #333;
  }

  .pl-ent {
    color: #63a35c;
  }

  .pl-k {
    color: #a71d5d;
  }

  .pl-pds,
  .pl-s,
  .pl-s .pl-pse .pl-s1,
  .pl-sr,
  .pl-sr .pl-cce,
  .pl-sr .pl-sra,
  .pl-sr .pl-sre {
    color: #183691;
  }

  .pl-v {
    color: #ed6a43;
  }

  .pl-id {
    color: #b52a1d;
  }

  .pl-ii {
    background-color: #b52a1d;
    color: #f8f8f8;
  }

  .pl-sr .pl-cce {
    color: #63a35c;
    font-weight: bold;
  }

  .pl-ml {
    color: #693a17;
  }

  .pl-mh,
  .pl-mh .pl-en,
  .pl-ms {
    color: #1d3e81;
    font-weight: bold;
  }

  .pl-mq {
    color: #008080;
  }

  .pl-mi {
    color: #333;
    font-style: italic;
  }

  .pl-mb {
    color: #333;
    font-weight: bold;
  }

  .pl-md {
    background-color: #ffecec;
    color: #bd2c00;
  }

  .pl-mi1 {
    background-color: #eaffea;
    color: #55a532;
  }

  .pl-mdr {
    color: #795da3;
    font-weight: bold;
  }

  .pl-mo {
    color: #1d3e81;
  }

  kbd {
    display: inline-block;
    padding: 3px 5px;
    font: 11px Consolas, "Liberation Mono", Menlo, Courier, monospace;
    line-height: 10px;
    color: #555;
    vertical-align: middle;
    background-color: #fcfcfc;
    border: solid 1px #ccc;
    border-bottom-color: #bbb;
    border-radius: 3px;
    box-shadow: inset 0 -1px 0 #bbb;
  }


  &:checked
    +.radio-label
      z-index: 1;
      position: relative;
      border-color: #4078c0;



/* desert scheme ported from vim to google prettify */
pre.prettyprint { display: block; background-color: #333; padding: 15px; }
pre .nocode { background-color: none; color: #000 }
pre .str { color: #ffa0a0 } /* string  - pink */
pre .kwd { color: #f0e68c; font-weight: bold }
pre .com { color: #87ceeb } /* comment - skyblue */
pre .typ { color: #98fb98 } /* type    - lightgreen */
pre .lit { color: #cd5c5c } /* literal - darkred */
pre .pun { color: #fff }    /* punctuation */
pre .pln { color: #fff }    /* plaintext */
pre .tag { color: #f0e68c; font-weight: bold } /* html/xml tag    - lightyellow */
pre .atn { color: #bdb76b; font-weight: bold } /* attribute name  - khaki */
pre .atv { color: #ffa0a0 } /* attribute value - pink */
pre .dec { color: #98fb98 } /* decimal         - lightgreen */

/* Specify class=linenums on a pre to get line numbering */
ol.linenums { margin-top: 0; margin-bottom: 0; color: #AEAEAE } /* IE indents via margin-left */
li.L0,li.L1,li.L2,li.L3,li.L5,li.L6,li.L7,li.L8 { list-style-type: none }
/* Alternate shading for lines */
li.L1,li.L3,li.L5,li.L7,li.L9 { }

@media print {
  pre.prettyprint { background-color: none }
  pre .str, code .str { color: #060 }
  pre .kwd, code .kwd { color: #006; font-weight: bold }
  pre .com, code .com { color: #600; font-style: italic }
  pre .typ, code .typ { color: #404; font-weight: bold }
  pre .lit, code .lit { color: #044 }
  pre .pun, code .pun { color: #440 }
  pre .pln, code .pln { color: #000 }
  pre .tag, code .tag { color: #006; font-weight: bold }
  pre .atn, code .atn { color: #404 }
  pre .atv, code .atv { color: #060 }
}

@media screen and (max-width: 1024px)
  .markdown-body
    font-size: 0.9em;
    img
      width: 100%;
    ul, ol
      font-size: 1.7em;
      li
        font-size: 0.6em;

    .side-menu
      display: none;
      h4
        margin-top: 0;

